    <listeners>
        <listener class-name="org.uncommons.reportng.HTMLReporter" />
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter" />
        <!-- <listener class-name="com.oem.framework.listeners.CustomReporter"/> -->
        
    </listeners>

    <test name="OEM Regression Tests">
      <classes>
      <class name="com.oem.Suits.FlexManagementSuite"/>
      <class name="com.oem.Suits.FlexReQuoteTenderTest"/>
