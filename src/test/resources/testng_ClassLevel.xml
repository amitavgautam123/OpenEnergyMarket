      <class name="com.oem.PortfolioManager.CompanyProfileTest"/>
      <class name="com.oem.QuotesAndTenders.RequestQuoteTest"/>
      <class name="com.oem.PortfolioManager.PropertyPortfolioMeterTest"/>
      <class name="com.oem.Suits.GasSuiteE2Etest"/>
      <class name="com.oem.PortfolioManager.PortfolioCalendarTest"/>
      <class name="com.oem.FixedProcurement.TenderRequoteTest"/>
      <class name="com.oem.suppliers.TendersAndAlertsTest"/>
      <class name="com.oem.VerifyTenders.VerifyTendersTest"/>
      
      <class name="com.oem.Suits.HHsuiteE2Etest"/>
      <class name="com.oem.Suits.FixedProcurementSuite"/>
      <class name="com.oem.Forcasting.ForecastingTest"/>
      <class name="com.oem.PortfolioManager.PropertyPortfolioTest"/>
      <class name="com.oem.FixedProcurement.ManageTendersTest"/>
      <class name="com.oem.FixedProcurement.TenderResponseTest"/>
      <class name="com.oem.Suits.NHHsuiteE2Etest"/>
      
      
    </classes>

    </test>


</suite>
